@prefix : <http://userprofile.com/owl/profile#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:UserProfile a rdfs:Class;
    rdfs:label "UserProfile" ;
    rdfs:comment "A user profile." .

:Interest a rdfs:Class;
    rdfs:label "Interest" ;
    rdfs:comment "Interest of a user." .

:hasInterest a rdf:Property ;
   rdfs:domain :UserProfile;
   rdfs:range :Interest .

:name a rdf:Property;
    rdfs:domain :UserProfile ;
    rdfs:range xsd:string .

:email a rdf:Property;
    rdfs:domain :UserProfile ;
    rdfs:range xsd:string .

:movieYearStart a rdf:Property;
    rdfs:domain :UserProfile ;
    rdfs:range xsd:integer .

:movieYearEnd a rdf:Property;
    rdfs:domain :UserProfile ;
    rdfs:range xsd:integer .

:genre a rdf:Property;
    rdfs:domain :Interest ;
    rdfs:range xsd:string .

:actor a rdf:Property;
    rdfs:domain :Interest ;
    rdfs:range xsd:string .

:country a rdf:Property;
    rdfs:domain :Interest ;
    rdfs:range xsd:string .

:user  rdf:type    :UserProfile .
:user  :name       "elvis" .
:user  :email      "emailelvis@gmail.com" .
:user  :movieYearStart     2020 .
:user  :movieYearEnd       2022 .
:user_interest rdf:type    :Interest .
:user  :hasInterest    :user_interest.
:user_interest :genre  "Comedy" .
:user_interest :genre  "Superhero" .
:user_interest :genre  "Thriller".
:user_interest :actor  "Tom Holland".
:user_interest :actor  "Tom Cruise".
:user_interest :country  "United States of America".
:user_interest :country  "South Korea".





